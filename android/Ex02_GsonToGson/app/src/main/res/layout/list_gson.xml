<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.ex02_gsontogson.GsonList"
    android:orientation="vertical">
    <EditText
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:hint="배열을 파라메터로 쓰기"
        android:id="@+id/et_arr1"/>
    <EditText
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:hint="배열을 파라메터로 쓰기"
        android:id="@+id/et_arr2"/>
    <EditText
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:hint="배열을 파라메터로 쓰기"
        android:id="@+id/et_arr3"/>
    <Button
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="전송하기"
        android:id="@+id/btn_send2"/>
    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:hint="데이터 받는곳"
        android:id="@+id/tv_arr1"/>
    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:hint="데이터 받는곳"
        android:id="@+id/tv_arr2"/>
    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:hint="데이터 받는곳"
        android:id="@+id/tv_arr3"/>
</LinearLayout>